{
  "manifest_version": 3,
  "name": "Notebook Pro",
  "version": "1.0",
  "description": "Organize your thoughts and save important bookmarks",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["storage"],
  "icons": {
    "128": "icon.png"
  }
}